{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "flutter_store_web_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--web-experimental-hot-reload",
            ]
        },
        {
            "name": "flutter_store_web_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--web-experimental-hot-reload",
            ]
        },
        {
            "name": "flutter_store_web_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--web-experimental-hot-reload",
            ]
        }
    ]
}